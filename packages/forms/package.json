{
    "name": "@medly-components/forms",
    "version": "2.2.12",
    "sideEffects": false,
    "main": "dist/cjs/index.js",
    "module": "dist/es/index.js",
    "types": "dist/es/index.d.ts",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "scripts": {
        "build": "cross-env BABEL_ENV=production rollup -c=../../rollup.config.js",
        "lint:css": "stylelint 'src/**/*.tsx'",
        "lint:fix": "run-p -c 'lint:css --fix' 'lint:ts --fix'",
        "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
        "lint": "run-p -c lint:css lint:ts",
        "prebuild": "rimraf dist && tsc",
        "test:watch": "yarn test --watch",
        "test": "tsc && jest --selectProjects forms --config ../../jest.config.js ",
        "watch": "yarn run build -w"
    },
    "peerDependencies": {
        "react": "16.x || 17.x",
        "react-dom": "16.x || 17.x",
        "styled-components": "^5.1.0"
    },
    "dependencies": {
        "@medly-components/core": "6.27.1",
        "@medly-components/theme": "5.2.1",
        "@medly-components/utils": "3.1.1",
        "date-fns": "^2.16.1",
        "fast-memoize": "^2.5.2"
    }
}
